{
  "kind": "collectionType",
  "collectionName": "months",
  "info": {
    "singularName": "month",
    "pluralName": "months",
    "displayName": "Month",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "month": {
      "type": "enumeration",
      "enum": [
        "january",
        "february",
        "march",
        "april",
        "may",
        "june",
        "july",
        "august",
        "september",
        "october",
        "november",
        "december"
      ],
      "required": true
    },
    "displayImg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "heroImg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "highlight": {
      "type": "component",
      "repeatable": false,
      "component": "shared.highlight",
      "required": true
    },
    "monthTag": {
      "type": "string",
      "required": true
    },
    "tagBlogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "tagMonths"
    },
    "tagDestinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "mappedBy": "tagMonths"
    },
    "tagExperiences": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experience.experience",
      "mappedBy": "tagMonths"
    }
  }
}
